<UserControl x:Class="VisGist.Views.GistsBrowser"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:VisGist.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <TreeView x:Name="GistsBrowserTV">
            <TreeView.Items>
                <TreeViewItem Header="Meat"></TreeViewItem>
                <TreeViewItem Header="Fruit"></TreeViewItem>
                <TreeViewItem Header="Spam"></TreeViewItem>
            </TreeView.Items>
        </TreeView>
    </Grid>
</UserControl>
